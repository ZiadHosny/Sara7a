{
    "version": 2,
    "name": "sara7a",
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**",
                    "dist/views",
                    "dist/public"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}